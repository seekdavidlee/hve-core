    "node_modules/**",
    "**/node_modules/**",
    "packages/**",
    "**/packages/**",
    "vendor/**",
    "**/vendor/**",
    "dist/**",
    "build/**",
    ".git/**",
    "**/.terraform/**",
    "**/.terraform.lock.hcl",
    ".vscode/**",
    ".github/copilot-instructions.md",
    "**/*.min.js",
    "**/*.min.css",
    ".mega-linter.yml",
